{
    "name": "Mobilizon",
    "id": "mobilizon",
    "packaging_format": 1,
    "description": {
        "en": "Alternative éthique aux événements, groupes et pages Facebook.",
        "fr": "As an ethical alternative to Facebook events, groups and pages."
    },
    "version": "1.2.1~ynh2",
    "url": "https://joinmobilizon.org",
    "upstream": {
        "license": "AGPL-V3",
        "website": "https://joinmobilizon.org",
        "demo": "https://demo.mobilizon.org",
        "admindoc": "https://docs.joinmobilizon.org/administration/configure",
        "userdoc": "https://docs.joinmobilizon.org/fr/utilisation",
        "code": "https://framagit.org/framasoft/mobilizon"
    },
    "license": "AGPL-V3",
    "maintainer": {
        "name": "llaq",
        "email": "contact@lelibreauquotidien.fr"
    },
    "requirements": {
        "yunohost": ">= 4.1.3"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "admin",
                "type": "user",
                "example": "johndoe"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "language",
                "type": "string",
                "ask": {
                    "en": "Choose the application language",
                    "fr": "Choisissez la langue de l'application"
                },
                "choices": ["fr", "en"],
                "default": "fr"
            },
            {
                "name": "password",
                "type": "password",
                "help": {
                    "en": "Mot de passe administrateur",
                    "fr": "Admin Password"
                },
                "example": "Choose a password"
            },
            {
                "name": "description",
                "type": "string",
                "ask": {
                    "en": "Choose the application description",
                    "fr": "Choisissez la description de l'application"
                }
            }
        ]
    }
}
